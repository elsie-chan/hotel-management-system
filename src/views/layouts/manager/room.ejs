<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Room Management</title>
    <link rel="icon" href="/images/icon.png" type="image/x-icon"/>
    <link rel="stylesheet" href="/css/styles.min.css"/>
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/tabler-icons/1.35.0/iconfont/tabler-icons.min.css"
          integrity="sha512-tpsEzNMLQS7w9imFSjbEOHdZav3/aObSESAL1y5jyJDoICFF2YwEdAHOPdOr1t+h8hTzar0flphxR76pd0V1zQ=="
          crossorigin="anonymous" referrerpolicy="no-referrer"/>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.0.1/css/toastr.css" rel="stylesheet"/>
</head>
<body>
<!--  Body Wrapper -->
<div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
     data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <aside class="left-sidebar">
        <!-- Sidebar scroll-->
        <div>
            <div class="brand-logo d-flex align-items-center justify-content-between">
                <a href="/manager/room" class="text-nowrap logo-img">
                    <img src="/images/Logo.png" width="150" alt=""/>
                </a>
                <div class="close-btn d-xl-none d-block sidebartoggler cursor-pointer" id="sidebarCollapse">
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-arrow-bar-left" width="20" height="20" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                        <path stroke="none" d="M0 0h24v24H0z" fill="none"/>
                        <path d="M4 12l10 0" />
                        <path d="M4 12l4 4" />
                        <path d="M4 12l4 -4" />
                        <path d="M20 4l0 16" />
                    </svg>
                </div>
            </div>
            <!-- Sidebar navigation-->
            <nav class="sidebar-nav scroll-sidebar" data-simplebar="">
                <ul id="sidebarnav">
                    <li class="nav-small-cap">
                        <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
                        <span class="hide-menu">MANAGER</span>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" href="/manager/dashboard" aria-expanded="false">
                            <span>
                              <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-home-2"
                                   width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor"
                                   fill="none" stroke-linecap="round" stroke-linejoin="round">
                                   <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                   <path d="M5 12l-2 0l9 -9l9 9l-2 0"></path>
                                   <path d="M5 12v7a2 2 0 0 0 2 2h10a2 2 0 0 0 2 -2v-7"></path>
                                   <path d="M10 12h4v4h-4z"></path>
                                </svg>
                            </span>
                            <span class="hide-menu">Dashboard</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" href="/manager/frontdesk" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-device-imac"
                                     width="24" height="24" viewBox="0 0 24 24" stroke-width="1.5" stroke="#2c3e50"
                                     fill="none" stroke-linecap="round" stroke-linejoin="round">
                                  <path stroke="none" d="M0 0h24v24H0z" fill="none"/>
                                  <path d="M3 4a1 1 0 0 1 1 -1h16a1 1 0 0 1 1 1v12a1 1 0 0 1 -1 1h-16a1 1 0 0 1 -1 -1v-12z"/>
                                  <path d="M3 13h18"/>
                                  <path d="M8 21h8"/>
                                  <path d="M10 17l-.5 4"/>
                                  <path d="M14 17l.5 4"/>
                                </svg>
                            </span>
                            <span class="hide-menu">Front desk</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link active" href="/manager/room" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-bed-filled" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                   <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                   <path d="M3 6a1 1 0 0 1 .993 .883l.007 .117v6h6v-5a1 1 0 0 1 .883 -.993l.117 -.007h8a3 3 0 0 1 2.995 2.824l.005 .176v8a1 1 0 0 1 -1.993 .117l-.007 -.117v-3h-16v3a1 1 0 0 1 -1.993 .117l-.007 -.117v-11a1 1 0 0 1 1 -1z" stroke-width="0" fill="currentColor"></path>
                                   <path d="M7 8a2 2 0 1 1 -1.995 2.15l-.005 -.15l.005 -.15a2 2 0 0 1 1.995 -1.85z" stroke-width="0" fill="currentColor"></path>
                                </svg>
                            </span>
                            <span class="hide-menu">Room</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" href="/manager/reservation" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-bookmark-filled" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                   <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                   <path d="M14 2a5 5 0 0 1 5 5v14a1 1 0 0 1 -1.555 .832l-5.445 -3.63l-5.444 3.63a1 1 0 0 1 -1.55 -.72l-.006 -.112v-14a5 5 0 0 1 5 -5h4z" stroke-width="0" fill="currentColor"></path>
                                </svg>
                            </span>
                            <span class="hide-menu">Reservation</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" href="/manager/invoice" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-file-dollar" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                   <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                   <path d="M14 3v4a1 1 0 0 0 1 1h4"></path>
                                   <path d="M17 21h-10a2 2 0 0 1 -2 -2v-14a2 2 0 0 1 2 -2h7l5 5v11a2 2 0 0 1 -2 2z"></path>
                                   <path d="M14 11h-2.5a1.5 1.5 0 0 0 0 3h1a1.5 1.5 0 0 1 0 3h-2.5"></path>
                                   <path d="M12 17v1m0 -8v1"></path>
                                </svg>
                            </span>
                            <span class="hide-menu">Invoice</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" href="/manager/feedback" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-message-report" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                   <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                   <path d="M4 21v-13a3 3 0 0 1 3 -3h10a3 3 0 0 1 3 3v6a3 3 0 0 1 -3 3h-9l-4 4"></path>
                                   <path d="M12 8l0 3"></path>
                                   <path d="M12 14l0 .01"></path>
                                </svg>
                            </span>
                            <span class="hide-menu">Feedback</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" href="/manager/service" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-plane-arrival" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                   <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                   <path d="M15.157 11.81l4.83 1.295a2 2 0 1 1 -1.036 3.863l-14.489 -3.882l-1.345 -6.572l2.898 .776l1.414 2.45l2.898 .776l-.12 -7.279l2.898 .777l2.052 7.797z"></path>
                                   <path d="M3 21h18"></path>
                                </svg>
                            </span>
                            <span class="hide-menu">Services</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link" href="/manager/meal" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-soup"
                                     width="24" height="24" viewBox="0 0 24 24" stroke-width="1.5" stroke="#2c3e50"
                                     fill="none" stroke-linecap="round" stroke-linejoin="round">
                                      <path stroke="none" d="M0 0h24v24H0z" fill="none"/>
                                      <path d="M4 11h16a1 1 0 0 1 1 1v.5c0 1.5 -2.517 5.573 -4 6.5v1a1 1 0 0 1 -1 1h-8a1 1 0 0 1 -1 -1v-1c-1.687 -1.054 -4 -5 -4 -6.5v-.5a1 1 0 0 1 1 -1z"/>
                                      <path d="M12 4a2.4 2.4 0 0 0 -1 2a2.4 2.4 0 0 0 1 2"/>
                                      <path d="M16 4a2.4 2.4 0 0 0 -1 2a2.4 2.4 0 0 0 1 2"/>
                                      <path d="M8 4a2.4 2.4 0 0 0 -1 2a2.4 2.4 0 0 0 1 2"/>
                                </svg>
                            </span>
                            <span class="hide-menu">Meal</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link logout" href="" aria-expanded="false">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-logout-2" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                   <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                   <path d="M10 8v-2a2 2 0 0 1 2 -2h7a2 2 0 0 1 2 2v12a2 2 0 0 1 -2 2h-7a2 2 0 0 1 -2 -2v-2"></path>
                                   <path d="M15 12h-12l3 -3"></path>
                                   <path d="M6 15l-3 -3"></path>
                                </svg>
                            </span>
                            <span class="hide-menu">Logout</span>
                        </a>
                    </li>
                </ul>

            </nav>
            <!-- End Sidebar navigation -->
        </div>
    </aside>
    <div class="body-wrapper">
        <!--  Header Start -->
        <header class="app-header">
            <nav class="navbar navbar-expand-lg navbar-light">
                <ul class="navbar-nav">
                    <li class="nav-item d-block d-xl-none">
                        <a class="nav-link sidebartoggler nav-icon-hover" id="headerCollapse" href="javascript:void(0)">
                            <i class="ti ti-menu-2"></i>
                        </a>
                    </li>
                </ul>
            </nav>
        </header>
        <div class="container-fluid">
            <div class="col-lg-12 d-flex align-items-stretch">
                <div class="card w-100">
                    <div class="card-body p-4">
                        <div class="row align-items-center">
                            <h5 class="col card-title fw-semibold">All rooms</h5>
                            <button type="button" class="col-2 btn btn-outline-primary m-1" data-bs-toggle="modal" data-bs-target="#addRoomModal">Add room</button>
                        </div>

                        <div class="table-responsive">
                            <table class="table-hover table text-nowrap mb-0 align-middle">
                                <thead class="text-dark fs-4">
                                <tr>
                                    <th class="border-bottom-0"></th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Number</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Information</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Price</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Facility</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Cleaning status</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Status</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                    </th>
                                </tr>
                                </thead>
                                <tbody>
                                <% rooms.forEach(function(room){ %>
                                <tr class="">
                                    <td class="border-bottom-0">
                                        <div class="d-flex px-2 py-1">
                                            <div>
                                                <img src="/uploads/rooms/<%= room.images? room.images : '101.png'%>" width="100" height="60" class="avatar avatar-sm" alt="room_img">
                                            </div>
                                        </div>
                                    </td>

                                    <td class="border-bottom-0"><h6 class="fw-semibold mb-0"><%= room.roomNumber %></h6></td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-normal mb-1"></h6>
                                        <span class="fw-normal mb-1"><%= room.roomFloor %></span>
                                    </td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-normal"><%= room.price %> VND</h6>
                                    </td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-normal"><%= room.facilities %></h6>
                                    </td>
                                    <td class="border-bottom-0">
                                        <div class="d-flex align-items-center gap-2">
                                            <span class="badge bg-success rounded-3 fw-semibold"><%= room.status%></span>
                                        </div>
                                    </td>
                                    <td class="border-bottom-0">
                                        <div class="d-flex align-items-center gap-2">
                                            <span class="badge <%= room.status==='Available'? 'bg-success': 'bg-warning'%> rounded-3 fw-semibold"><%= room.isAvailable %></span>
                                        </div>
                                    </td>
                                    <td class="border-bottom-0">
                                        <div class="d-flex align-items-center gap-2 cursor-pointer" data-bs-toggle="dropdown">
                                            <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-dots" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                                <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                                <path d="M5 12m-1 0a1 1 0 1 0 2 0a1 1 0 1 0 -2 0"></path>
                                                <path d="M12 12m-1 0a1 1 0 1 0 2 0a1 1 0 1 0 -2 0"></path>
                                                <path d="M19 12m-1 0a1 1 0 1 0 2 0a1 1 0 1 0 -2 0"></path>
                                            </svg>
                                        </div>
                                        <div class="dropdown-menu dropdown-menu-end dropdown-menu-animate-up" aria-labelledby="drop2">
                                            <div class="message-body">
                                                <a href="#" class="update-room d-flex align-items-center gap-2 dropdown-item" data-bs-toggle="modal" data-bs-target="#editRoomModal" data-id="<%= room.id %>">
                                                    <p class="mb-0 fs-3">Edit</p>
                                                </a>
                                                <a href="#" class="delete-room d-flex align-items-center gap-2 dropdown-item" data-id="<%= room.id %>">
                                                    <p class="mb-0 fs-3">Hide</p>
                                                </a>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                                <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <nav aria-label="Page navigation example">
                        <ul class="pagination justify-content-center">
                            <li class="page-item <%= pagination.page == 1 ? 'disabled' : ''%>">
                                <a class="page-link"
                                   href="/manager/room/?page=<%= +pagination.page -1 && 1 %>"
                                   aria-label="Previous">
                                    <span aria-hidden="true">&laquo;</span>
                                </a>
                            </li>
                            <% for (let i = 1; i <= pagination.pages; i++) {%>
                                <li class="page-item <%= i == (pagination.page) && 'active'%>">
                                    <a class="page-link" href="/manager/room/?page=<%= i %>"
                                    ><%= i %></a>
                                </li>
                            <% }%>
                            <li class="page-item <%= pagination.page == pagination.pages ? 'disabled' : ''%>">
                                <a
                                        class="page-link"
                                        href="/manager/room/?page=<%= +pagination.page  + 1 %>"
                                        aria-label="Next"
                                >
                                    <span aria-hidden="true">&raquo;</span>
                                </a>
                            </li>
                        </ul>
                    </nav>

                </div>
            </div>
        </div>
        <!-- Add room modal -->
        <div class="modal fade" id="addRoomModal" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="">Add new room</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true"></span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="d-flex flex-row justify-content-center row">
                            <div class="image_inputs col-10">
                                <div class="row align-items-center">
                                    <div class="image_container col-4">
                                        <label class="col mt-1 " for="room_image--add"
                                               style="height: fit-content;">
                                            <img id="image_preview" class="card-hover image_upload" width="100%"
                                                 height="100%" src="/images/img_1.jpg" alt="room image uploaded">
                                            <input name="image" type="file" id="room_image--add" accept="image/png, image/jpge, image/jpg" class="d-none">
                                        </label>
                                    </div>
                                    <div class="col text">
                                        <span>Upload image</span>
                                    </div>
                                </div>
                            </div>

                            <div class="col-10">
                                <div class="d-flex flex-row justify-content-between row mb-2">
                                    <label for="room_number--add" class="col-4 col-md-4 col-form-label">Room Number</label>
                                    <div class="col">
                                        <input type="number" class="form-control" id="room_number--add" placeholder="Enter room number">
                                    </div>
                                </div>
                                <div class="mb-3 row">
                                    <div class="col">
                                        <label for="room_type--add" class="col-sm-6 col-form-label">Room type</label>
                                        <div class="col-sm-12">
                                            <select class="form-select " id="room_type--add" >
                                                <option value="" disabled selected hidden>Choose room type</option>
                                                <% categories.forEach(function(category) { %>
                                                <option value="<%= category._id %>"><%= category.name %></option>
                                                <% }) %>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <label for="room_floor--add" class="col-sm-6 col-form-label">Room floor</label>
                                        <div class="col-sm-12">
                                            <input type="text" min="0" class="form-control"
                                                   id="room_floor--add"
                                                   placeholder="Enter room floor"
                                                   aria-label=""
                                                   aria-describedby="addon-wrapping">
                                        </div>
                                    </div>
                                </div>
                                <div class="mb-3 row">
                                    <div class="col">
                                        <label for="room_cleaning-status--add" class="col col-form-label">Cleaning status</label>
                                        <div class="col-sm-12">
                                            <select class="form-select" id="room_cleaning-status--add">
                                                <option value="" disabled selected hidden>Choose cleaning status</option>
                                                <option value="Clean">Clean</option>
                                                <option value="Cleaning">Cleaning</option>
                                                <option value="Dirty">Dirty</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <label for="room_status--add" class="col-sm-6 col-form-label">Status</label>
                                        <div class="col-sm-12">
                                            <select class="form-select" id="room_status--add">
                                                <option value="" disabled selected hidden>Choose status</option>
                                                <option value="Available">Available</option>
                                                <option value="Maintained">Maintained</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="d-flex flex-row justify-content-between row mb-2">
                                    <label for="room_price--add" class="col-4 col-md-4 col-form-label">Price</label>
                                    <div class="col">
                                        <input type="text" class="form-control" id="room_price--add">
                                    </div>
                                </div>
                                <div class="d-flex flex-row justify-content-between row mb-2">
                                    <label for="room_facilities--add" class="col-4 col-md-4 col-form-label">Facility</label>
                                    <div class="col-12">
                                        <input type="text" class="form-control" id="room_facilities--add">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn " data-bs-dismiss="modal">Close</button>
                        <button type="button" class="addRoomBtn btn btn-primary">Add</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- Add room modal -->
        <!-- Edit room modal -->
        <div class="modal fade modal-lg" id="editRoomModal" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="">Edit room</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true"></span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="d-flex flex-row justify-content-center row">
                            <div class="image_inputs col-10">
                                <div class="row align-items-center">
                                    <div class="image_container col-4">
                                        <label class="col mt-1 " for="image_upload--edit"
                                               style="height: fit-content;">
                                            <img id="edit--image_preview" class="card-hover image_upload" width="100%"
                                                 height="100%" src="/images/img_1.jpg" alt="room image uploaded">
                                            <input type="file" id="image_upload--edit" accept="image/png, image/jpge, image/jpg" class="visually-hidden">
                                        </label>
                                    </div>
                                    <div class="col text">
                                        <span>Upload image</span>
                                    </div>
                                </div>
                            </div>

                            <div class="col-10">
                                <div class="d-flex flex-row justify-content-between row mb-2">
                                    <label for="edit--room_number" class="col-4 col-md-4 col-form-label">Room Number</label>
                                    <div class="col">
                                        <input type="number" class="form-control" id="edit--room_number" placeholder="Enter room number">
                                    </div>
                                </div>
                                <div class="mb-3 row">
                                    <div class="col">
                                        <label for="edit--room_type" class="col-sm-6 col-form-label">Room type</label>
                                        <div class="col-sm-12">
                                            <select class="form-select" id="edit--room_type">
                                                <option value="" disabled selected hidden>Choose room type</option>
                                                <% categories.forEach(function(category) { %>
                                                    <option value="<%= category._id %>"><%= category.name %></option>
                                                <% }) %>
                                            </select>
                                        </div>
                                    </div>
